    function () {
      let value = $(this).val();
      if (value == "USD") {
        location.href = "https://www.moxielash.com";
        return;
      }

      let bottomCurrency = $(
        ".locale-selectors__container .locale-selectors__selector"
      );
      bottomCurrency.val(value);
      bottomCurrency.closest("form").submit();
    }
  );
  $(document).ready(function () {
    function findGeoLocation(d) {
      console.log("geo-ip", d);
      window.current_country = d.country_code2;

      if (d.country_code2) {
        localStorage.setItem("geo_location", JSON.stringify(d));
      }
      if (d.country_code2 == "US") {
        setInterval(function () {
          if ($(".show-if-US").hasClass("hidden")) {
            $(".show-if-US").removeClass("hidden");
            $(".show-if-not-US").addClass("hidden");
          }
        }, 100);
        $("body").addClass(`country-found-${d.country_code2}`);
        $(document).trigger("current-country", [d.country_code2]);
      } else {
        setInterval(function () {
          if ($(".show-if-not-US").hasClass("hidden")) {
            $(".show-if-not-US").removeClass("hidden");
            $(".show-if-US").addClass("hidden");
          }
        }, 100);
        $("body").addClass(`country-found-${d.country_code2}`);
        $(document).trigger("current-country", [d.country_code2]);
        $(document).trigger("current-country", [d.country_code2]);
      }
    }

    var GeoLocation = localStorage.getItem("geo_location");
    if (GeoLocation && GeoLocation != "") {
      GeoLocation = JSON.parse(GeoLocation);
      findGeoLocation(GeoLocation);
    } else {
      $.getJSON(
        "https://api.ipgeolocation.io/ipgeo?apiKey=717f09b07c074e04959cd1a144f6935c",
        findGeoLocation
      );
    }

    let findCurrencytLocation = setInterval(() => {
      if ($(".locale-selectors__selector").length > 0) {
        clearInterval(findCurrencytLocation);
        let val = $(".locale-selectors__selector").val();
        let b = $(".locale-selectors__selector").clone();

        $(".currency-geo-wrapper").html(b);
        $(".current-currency").text(val);
        $(".currency-geo-wrapper .locale-selectors__selector").val(val);
      }
    }, 100);
  
