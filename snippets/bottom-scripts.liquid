<script>
  jQuery(document).ready(function () {
    let CounterTemplate = (hour, minutes, seconds) => {
      return `<div class="flex" style="display:flex!important;" > 
  
  <span style="display:none!important">[TIMER]</span>
  <div>     <span class="pf-countdown__mins">${hour}</span> <label>Hours</label>
  </div>
  <div class="pf-countdown__colon" style="    padding: 10px;"><span>:</span></div>
  
  <div><span class="pf-countdown__mins">${minutes}</span><label>Mins</label></div>
  <div class="pf-countdown__colon" style="    padding: 10px;"><span>:</span></div>
  <div><span class="pf-countdown__secs">${seconds}</span><label>Secs</label></div>
  
  </div>`;
    };

    function startTimer(duration, display) {
      var timer = duration,
        minutes,
        seconds;
      let hr = 2;
      mm = 00;
      ss = 60;
      var interval = setInterval(function () {
        if (hr == 0 && mm == 1 && ss == 0) clearInterval(interval);
        ss--;
        if (ss == 0) {
          ss = 59;
          mm--;
          if (mm <= 0) {
            mm = 59;
            hr--;
          }
        }

        if (hr.toString().length < 2) hr = "0" + hr;
        if (mm.toString().length < 2) mm = "0" + mm;
        if (ss.toString().length < 2) ss = "0" + ss;

        display.html(CounterTemplate(hr, mm, ss));
      }, 1000);
    }

    let SuperTimer = setInterval(() => {
      let TimerElement = $(".ql-container:contains('[TIMER]')");
      if (TimerElement.length > 0) {
        clearInterval(SuperTimer);
        var TMins = 48 * 60 * 1;
        startTimer(TMins, TimerElement);
      }
      if (window.debug) console.log("wating SuperTimer");
    }, 100);
  });
</script>
