{%- assign currentProduct = product -%} 
{%- if currentProduct.tags contains
'material_Cruelty-free' -%} {%- assign currentProductIsCruelty = true -%} {%-
else -%} {%- assign currentProductIsCruelty = false -%} {%- endif -%}
{% assign productsSize=0 %}

<div
  data-collection="{{ product_collection.handle }}"
  isCruelty="{{ currentProductIsCruelty }}"
  class="pf-0c37e1ff pf-r pf-r-eh sc-daURTG iUdgQL collection-slider-main {{class}}"
  data-pf-type="Row" 
> 
  {%- for product in product_collection.products  limit: 20 -%}
   {%- assign skipProduct = false -%}
   {%- if product.tags contains 'material_Cruelty-free' -%}
    {%- assign
  innerProductIsCruelty = true -%} 
  {%- else -%}
   {%- assign innerProductIsCruelty = false -%} 
  {%- endif -%} 
  {%- if currentProductIsCruelty == true and innerProductIsCruelty == false -%}
  
  {%- assign skipProduct = true -%} 
  {%- elsif currentProductIsCruelty == false and innerProductIsCruelty == true -%}
   {%- assign skipProduct = true -%} 
  {%- endif -%} 
  {% assign pttag= product.tags | join:', ' %} 
  <!-- pttag {{pttag}} -->
  {% if pttag  contains 'no-search' %}
  {%- assign skipProduct = true -%}
  {% endif %}
  {%- if currentProduct.id == product.id -%} 
  {%- assign skipProduct = true -%}
  {%- endif -%} 
  {%- unless currentProduct.available  -%} 
  {%- assign skipProduct = true -%}
  {%- endunless -%}  
  {%- unless skipProduct -%}
      {% assign productsSize= productsSize| plus:1 %}
  <div tags="{{pttag}} "
    isCruelty="{{ innerProductIsCruelty }}" 
    class="pf-c pf-c-xs--12 pf-c-sm--4 pf-c-md--3 single-product-container"
    style="padding: 15px;"
  >
  {%- assign single_class= "w-full mb-8"-%} {%- include 'single-product-item',
  class: single_class, img_url: product.images[0],pTitle:product.title ,
  pDesc:product.description,product_handle:
  product.handle,vId:0,pPrice:product.,pVendor:product.vendor
  ,variants:product -%}
  </div>
  {%- endunless -%} 
  
  {%- endfor -%}
  
  {% if productsSize == 0 %}
  {%- for product in product_collection.products  limit: 20 -%}
  {%- assign skipProduct = false -%}
  {% assign pttag= product.tags | join:', ' %} 
  <!-- pttag {{pttag}} -->
  {% if pttag  contains 'no-search' %}
  {%- assign skipProduct = true -%}
  {% endif %}
  {%- unless currentProduct.available  -%} 
  {%- assign skipProduct = true -%}
  {%- endunless -%} 
  {%- if currentProduct.id == product.id -%} 
  {%- assign skipProduct = true -%}
  {%- endif -%} 
 {%- unless skipProduct -%}
 <div
 tags="{{pttag}} "
   isCruelty="{{ innerProductIsCruelty }}" 
   class="pf-c pf-c-xs--12 pf-c-sm--4 pf-c-md--3 single-product-container"
   style="padding: 15px;"
 >
 {%- assign single_class= "w-full mb-8"-%} {%- include 'single-product-item',
 class: single_class, img_url: product.images[0],pTitle:product.title ,
 pDesc:product.description,product_handle:
 product.handle,vId:0,pPrice:product.,pVendor:product.vendor
 ,variants:product -%} 
 </div>
 {%- endunless -%}  
 
 {%- endfor -%}

  {% endif %}
 
