
{% liquid 
  assign product_col=null
  for t in product.tags 
  if t contains 'recommend'
  assign product_col= t | replace: 'recommend_':''
  break
  endif 
  endfor 
  assign product_collection= collections[product_col]
  
  unless product_col  
  assign product_collection= collections['lashes']
  endunless

  %}

   
 
