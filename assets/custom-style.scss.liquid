body.template-product {
  h3.section-header__title {
    font-family: Montserrat-Medium;
    font-size: 30px;
    color: #000000;
    letter-spacing: 0;
    text-align: center;
  }
  p.afterpay-paragraph,
  a.afterpay-link {
    color: #a0a0a0;
    font-size: 14px;
  }
  div.simple-tabs-wrapper {
    .tab {
      overflow: hidden;
      // border: 1px solid #ccc;
      background-color: white;
      border-bottom: 1px solid #ccc;
    }

    /* Style the buttons that are used to open the tab content */
    .tab button {
      background-color: inherit;
      float: left;
      border: none;
      outline: none;
      cursor: pointer;
      padding: 14px 16px;
      transition: 0.3s;
    }

    /* Change background color of buttons on hover */
    .tab button:hover {
      background-color: #ddd;
    }

    /* Create an active/current tablink class */
    .tab button.active {
      background-color: white;
      border: 1px solid #ccc;
      border-bottom: none;
    }

    /* Style the tab content */
    .tabcontent {
      display: none;
      padding: 6px 12px;
      //  border: 1px solid #ccc;
      border-top: none;
      p {
        font-family: futura,Brandon,sans-serif;
        font-size: 14px;
        color: #000000;
        letter-spacing: 0.29px;
        line-height: 25px;
      }
    }
  }
}
